{
  "name": "abyssal",
  "version": "0.1.0",
  "private": true,
  "author": "Aldo R. Robles",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build:db": "prisma generate && prisma db push",
    "build:next": "next build",
    "build": "run-s build:*",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|css|ts|tsx)\"",
    "format": "npm run prettier -- --list-different",
    "format:write": "npm run prettier -- --write",
    "typecheck": "tsc -b",
    "validate": "run-s typecheck lint"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.1",
    "@radix-ui/react-popover": "^1.0.5",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@types/node": "20.2.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.3",
    "next": "13.4.3",
    "next-auth": "^4.22.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-wrap-balancer": "^0.5.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=18",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.14.1"
  }
}
